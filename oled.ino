uint16_t cnt;
uint8_t step;
uint8_t drawMode;

uint8_t hour;
uint8_t minute;

unsigned long TTL = 60000;
unsigned long drawTime;

void setup() {
        Serial.begin(9600);

        // Clear the buffer.
        display.begin(SSD1306_SWITCHCAPVCC);
        display.clearDisplay();
        display.display();

        step = 1;
        cnt = 0;
        drawTime = 0;

        Time.zone(1);

        drawBootScreen();
        delay(6000);
        display.clearDisplay();

}

int inetDraw(String data) {
        while(data.length() > 0) {
                int x = data.substring(0, 3).toInt();
                int y = data.substring(3, 5).toInt();
                int value = data.substring(5,6).toInt();
                display.drawPixel(x,y,value);
                data = data.substring(6);
        }
        setDrawMode();
        display.display();
        return 1;
}

int inetClear(String notUsed) {
        setDrawMode();
        display.clearDisplay();
        display.display();
        return 0;
}

void setDrawMode() {
        drawTime = millis();
        step = 1;
        cnt = 0;
}

void loop() {
        if (drawTime > 0 && drawTime + TTL > millis()) {
                delay(1000);
                return;
        }
        delay(5);
        if (updateTime()) {
                display.clearDisplay();
                drawClock();
        }
//        scrollNetset();
}

void scrollNetset(){
        if (step == 1) {
                if (cnt == BITMAP_WIDTH*8) {
                        cnt = 0;
                        step = 2;
                        return;
                }
                drawFrame(cnt);
                cnt++;
        }
        if (step == 2) {
                if (cnt == SSD1306_LCDWIDTH) {
                        cnt = 0;
                        step = 3;
                        return;
                }
                drawBlackFrame();
                cnt++;
        }
        if (step == 3) {
                delay(3000);
                cnt = 0;
                step = 1;
        }
}

void drawBootScreen() {
        display.drawXBitmap(36, 10, kickstart10_bootscreen_bits,
                            kickstart10_bootscreen_width,
                            kickstart10_bootscreen_height, 1);
        display.display();
}

void drawFrame(uint16_t cnt) {
        display.shiftDisplayLeft();
        drawColumn(cnt);
        display.display();
}

void drawBlackFrame() {
        display.shiftDisplayLeft();
        display.display();
}

void drawColumn(int16_t x) {
        uint8_t y, data;

        for (y=0; y<SSD1306_LCDHEIGHT; y++) {
                data = bitmap_bits[(x/8) + y * BITMAP_WIDTH] & 1<<x%8;
                display.drawPixel(SSD1306_LCDWIDTH-1, y, data>0 ? 1 : 0);
        }
}

boolean updateTime(){
        uint8_t lHour = Time.hour();
        uint8_t lMinute = Time.minute();
        if (hour != lHour || minute != lMinute) {
                hour = lHour;
                minute = lMinute;
                return true;
        }
        return false;
}

void drawClock() {
        drawNumber(3, hour/10);
        drawNumber(31, hour%10);
        drawColon();
        drawNumber(72, minute/10);
        drawNumber(100, minute%10);
        display.display();
}

void drawNumber(uint8_t xpos, uint8_t number) {
        uint8_t* index = numbers_bits + (number * numbers_width * numbers_height / 8);
        display.drawXBitmap(xpos, 6, index, numbers_width, numbers_height, 1);
}

void drawColon() {
        display.drawXBitmap(60, 20, colon_bits, colon_width, colon_height, 1);
}
